{
  "name": "example-nestjs",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "**/*.scss"
  ],
  "main": "index.js",
  "scripts": {
    "build": "cross-env BUILD=true rspack build",
    "dev": "killport 3000 && rspack serve",
    "start": "node dist/main.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:coverage": "vitest run --coverage.enabled --coverage.all &&istanbul-badges-readme"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.5",
    "@nestjs/core": "^11.0.5",
    "@nestjs/platform-express": "^11.0.5",
    "@nestjs/schedule": "^5.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.7.9",
    "cache-manager": "^6.4.0",
    "class-validator": "^0.14.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "run-script-webpack-plugin": "0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.4",
    "@rspack/cli": "1.2.2",
    "@rspack/core": "1.2.2",
    "@vitest/coverage-istanbul": "^3.0.4",
    "@vitest/ui": "^3.0.4",
    "cross-env": "7.0.3",
    "istanbul-badges-readme": "^1.9.0",
    "unplugin-swc": "^1.5.1",
    "vitest": "^3.0.4"
  }
}
